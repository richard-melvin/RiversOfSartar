@startuml weaponsAndRunes.svg
!includeurl ../images/icons.puml

skinparam rectangleFontSize 24

rectangle "<$air*2.0,color=CadetBlue>" as air

rectangle "<$fire_sky*2.0,color=Salmon>" as fire_sky
rectangle "<$water*2.0,color=MediumSeaGreen>" as water
rectangle "<$earth*2.0,color=Sienna>" as earth
rectangle "<$darkness*2.0,color=DarkSlateGray>" as darkness


darkness .. earth
darkness .. fire_sky
earth .. air
fire_sky .. water
air . water


rectangle "Swords,\nDaggers" as sword
rectangle "Hammers,\nMaces" as club
rectangle "Flexible \nWeapons" as net
rectangle "Axes" as axe
rectangle "Bows,\nCrossbows" as bow
rectangle "Spears,\nClubs" as spear



air .. sword
darkness .up. club
water .. net
earth .le. axe

fire_sky . bow

darkness .. spear
earth . spear 
spear . fire_sky
spear .. air
spear .. water


rectangle "Pole Weapons" as pole
rectangle Shields
rectangle "Javelins,\nDarts" as javelin
rectangle Unarmed
rectangle Slings

pole . darkness
pole .. earth

darkness . Slings 
Slings .. fire_sky

air .. javelin
water .. javelin


earth .. Shields
Shields . air

fire_sky .. Unarmed
Unarmed .le. water


@enduml


